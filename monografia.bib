author = {de Almeida, Stef\^{a}nia Ordov\'{a}s and Mazzon, Jos\'{e} Afonso and Dholakia, Utpal and {M\"{u}ller Neto}, Hugo Fridolino},
booktitle = {Revista de Administra\c{c}\~{a}o Contempor\^{a}nea},
